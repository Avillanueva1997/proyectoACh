<core:FragmentDefinition
    xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:tnt="sap.tnt"
	xmlns:core="sap.ui.core">   
    
    <tnt:ToolHeader>
        <Button icon="sap-icon://home" type="Transparent" press="onHomePress" tooltip="Home">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <Image src="assets/images/AC_Logo.png" width="9rem">
        </Image>
        <ToolbarSpacer />
        <Button icon="sap-icon://group" type="Transparent" tooltip="Equipo" press="onTeam">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <!--<Button icon="sap-icon://add-activity-2" type="Transparent" tooltip="Plan de AcciÃ³n">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>-->
        <Button icon="sap-icon://business-objects-experience" type="Transparent" tooltip="Indicadores" press="onKpi">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
        <Button text="Alan Smith" type="Transparent" press="onUserNamePress">
            <layoutData>
                <OverflowToolbarLayoutData priority="NeverOverflow" />
            </layoutData>
        </Button>
