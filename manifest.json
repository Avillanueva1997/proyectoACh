                "viewId" : "viewSPRO04",
                "viewLevel" : 3	
            },
            "spro05": {
                "path": "sap.ui.app01.view",
                "viewName": "SPRO/SPRO05",
                "viewId" : "viewSPRO05",
                "viewLevel" : 3	
            },
            "spro06": {
                "path": "sap.ui.app01.view",
                "viewName": "SPRO/SPRO06",
                "viewId" : "viewSPRO06",
                "viewLevel" : 3	
            }
		}
	}
  }
}
